	{
		"description": "Regressions",
		"it": "GH-1093: Undefined helper context",
		"template": "{{#each obj}}{{{helper}}}{{.}}{{/each}}",
		"data": {
			"obj": {
				"bar": "bat"
			}
		},
		"expected": "notfoundbat",
		"helpers": {
			"helper": {
				"!code": true,
				"javascript": "function () {\n        // It's valid to execute a block against an undefined context, but\n        // helpers can not do so, so we expect to have an empty object here;\n        for (var name in this) {\n          if (this.hasOwnProperty(name)) {\n            return 'found';\n          }\n        }\n        // And to make IE happy, check for the known string as length is not enumerated.\n        return (this == 'bat' ? 'found' : 'not');\n      }"
			}
		},
		"compileOptions": {}
